{
  "manifest_version": 2,

  "name": "Eat the cookie, Astro",
  "description": "This extension eats the poorly designed Astro cookies",
  "version": "1.0",
  
  "background": {
  	"scripts": ["background.js"],
  	"persistent": false
  },

  "page_action": {
    "default_icon": "icon.png",
    "defulat_popup": "popup.html"
  },
  
  "permissions": [
    "cookies",
    "tabs",
    "*://log.planesense.com/*"
  ]
}